<div id="template-promo-next-prev">
  [[#is_next]]
    <span class="next-item-title">
      {{ i18n.site.object.next-item }}
    </span>
  [[/is_next]]
  [[#is_prev]]
    <span class="previous-item-title">
      {{ i18n.site.object.previous-item }}
    </span>
  [[/is_prev]]
  <div class="channel-object-next-prev collections-promo-item separator-after[[#is_next]] object-next[[/is_next]][[#is_prev]] object-prev[[/is_prev]]">
    <div class="content-outer">
      <div class="content-inner">
        <div class="content-outer">
          <div class="content-inner">
            [[^img]]
              <a href="[[url]]"></a>
            [[/img]]
            [[#img]]
              <a href="[[url]]" class="bg-link" style="background-image:url('[[src]]');"></a>
            [[/img]]
          </div>
        </div>
        {{> js_template/cho/promotions/channel-object-promo-text-js}}
      </div>
    </div>
  </div>
</div>
