<div class="data-section cf{{#extended_information}} closed{{/extended_information}}"{{#id}} data-section-id="{{.}}"{{/id}}>
  <h3 class="subsection-label">
    {{ title }}
    {{#info}}
      <a class="eu-tooltip-anchor" href="{{.}}">
        <svg class="icon icon-help">
          <use xlink:href="#icon-help"/>
        </svg>
      </a>
    {{/info}}
  </h3>
  <div class="subsection-content">

    <ul class="data-group">
      {{#sections}}
        <li>
          <section>
            
            {{#geo}}
              <div class="map-wrapper">  
                <a class="scroll-trigger"
                 id="scroll-trigger-map"
                 data-enabled="true"
                 data-fire-on-open="showMap"></a>
                <a id="map" style="bottom: 4.8em; position: relative;"></a>
                <div class="map">
                  <div class="markers">
                    {{#items}}
                      <a data-latitude="{{extra_info.latitude}}" data-longitude="{{extra_info.longitude}}" data-label="{{text}}"></a>
                    {{/items}}
                  </div>
                </div>
                <a class="scroll-trigger trigger-chain"
                    data-enabled="true"
                    data-disable-when-fired="showMap"
                    data-fire-on-open-params='{"trigger": "scroll-trigger-map"}'></a>
              </div>
            {{/geo}}

            {{#title }}<h4 class="data-header">{{.}}:</h4>{{/title }}
            <ul class="data-group {{#geo}}data-group-block{{/geo}} {{^geo}}comma-list{{/geo}}">
              {{# items }}
                <li {{#is_desc}}class="split-items"{{/is_desc}}>

                  {{#url }}<a href="{{.}}">{{/url }}
                    {{#ga_data}}<span class="ga-data" data-ga-metric="{{.}}">{{/ga_data}}
                      {{#raw}}{{{text}}}{{/raw}}
                      {{^raw}}{{text}}{{/raw}}
                    {{#ga_data}}</span>{{/ga_data}}
                  {{#url }}</a>{{/url }}

                  {{#extra_info}}
                    {{#life}}
                      <span class="life-dates">
                        (<time>{{from.short}} {{#to.short}} â€” {{.}}{{/to.short}}</time>)
                      </span>
                    {{/life}}

                    {{#biography}}
                      <div class="biography">
                        {{text}}
                      </div>
                    {{/biography}}
                  {{/extra_info}}
                  </li>
              {{/ items}}
            </ul>
          </section>
        </li>
      {{/sections}}
    </ul>
  </div>
</div>