{{#social}}
  <section class="channel-social section">
    <h4>{{social_title}}:</h4>
    <ul class="social-share">
      {{#twitter}}
        <li>
          <a class="icon-soundcloud" {{#url}}href="{{.}}"{{/url}} {{#text}}title="{{.}}"{{/text}} target="_blank">
            <svg class="icon icon-twitter">
              <use xlink:href="#icon-twitter"></use>
            </svg>
            {{#text}}<span class="share-name is-vishidden">{{.}}</span>{{/text}}
          </a>
        </li>        
      {{/twitter}}
