<div class="container-large" {{#object_id}}id="{{.}}"{{/object_id}}>
	<div class="ponent {{depth}}">
		<div class="widget-collapsed flex-container column" {{#object_id}}object_id="{{.}}"{{/object_id}}>
			<div class="mapping-card-controls">
				<div class="card-label normal-colored indent flex-item color-blue">{{prefix}}:{{name}}</div>
				<div class="card-label normal-colored-dark indent">{{xpath}}</div>
                <div class="flex-container ">
                    <div class="normal-colored values-collapse card-button">Values</div>
                    <div class="normal-colored mapping-card-function card-button"><span
                            class="mapping-card-f">f</span><span class="mapping-card-x">x</span></div>
                    <div class="normal-colored if card-button">if</div>
                </div>
			</div>
		</div>
		<div class="flex-container row" {{#object_id}}object_id="{{.}}"{{/object_id}}>
			<div class="mapping-card-values">
				<div class="mapping-field-item-container">
					{{#field_value_cells}}
						<div  class="mapping-field-item color-orange">
							{{>molecules/pandora/field-value}}
						</div>
					{{/field_value_cells}}
				</div>
			</div>
		</div>
	 </div>
	 <div class="mapping-card-dropdown">
		 {{#dropdown}}{{>molecules/controls/dropdown-menu}}{{/dropdown}}
	 </div>
</div>