{{#is_section}}
  </section>
  <noscript>
    <a class="ve-anchor no-js" id="ve-anchor-{{id}}"></a>
  </noscript>
  <section class="ve-slide {{#is_first}}first{{/is_first}}">
    <a class="ve-anchor" id="ve-anchor-{{id}}-js"></a>
{{/is_section}}

{{^is_section}}

  {{#is_ripple}}

    <div id="{{id}}" class="ve-base ve-base-ripples ve-{{id}}">

      {{#image}}
        {{>organisms/virtual-exhibitions/main-slide-bg-style}}
      {{/image}}

      {{#image}}
        <picture class="ve-intro-image">
          {{>atoms/images/responsive-srcset-picture}}

          {{#image_credit}}
            <div class="ve-image-credit">
             {{>molecules/text/attribution}}
            </div>
          {{/image_credit}}

        </picture>
      {{/image}}

        <div class="brand-circles">
          <div class="scaling-svg-container">
            <svg preserveAspectRatio="xMidYMin slice" viewBox="300 860 300 300">
              <path d="M600,860h-31.6c0,148.1-120.3,268.4-268.4,268.4v31.6C465.3,1160,600,1025.3,600,860z"/>
              <path d="M547,860h-31.6c0,118.9-96.8,215.4-215.4,215.4v31.6C436.1,1107,547,996.1,547,860z"/>
              <path d="M493.8,860h-31.6c0,89.5-72.7,162.2-162.2,162.2v31.6C407,1053.8,493.8,966.7,493.8,860z"/>
              <path d="M440.8,860h-31.6c0,60-48.9,109.2-109.2,109.2v31.6C377.6,1000.8,440.8,937.6,440.8,860z"/>
              <path d="M387.8,860h-31.6c0,30.9-25.3,56.2-56.2,56.2v31.6C348.4,947.8,387.8,908.4,387.8,860z"/>
            </svg>
          </div>
        </div>
      </div>
  {{/is_ripple}}

  {{#is_quote}}
    <div id="{{id}}" class="ve-base ve-base-quote {{type}} {{#is_full_section_element}}ve-full-height{{/is_full_section_element}}"{{#alchemy_id}} data-alchemy-element="{{.}}"{{/alchemy_id}}>
      {{#is_full_section_element}}
        <div class="centered-v-text">
          <div class="centered-text">
      {{/is_full_section_element}}

      {{>atoms/text/quote}}

      {{#is_full_section_element}}
          </div>
        </div>
      {{/is_full_section_element}}
    </div>
  {{/is_quote}}

  {{#is_text}}
    <div id="{{id}}" class="ve-base ve-base-text {{type}}"{{#alchemy_id}} data-alchemy-element="{{.}}"{{/alchemy_id}}>
      {{{body}}}
    </div>
  {{/is_text}}

  {{#is_embed}}
    <div id="{{id}}" class="ve-base ve-base-embed {{type}}"{{#alchemy_id}} data-alchemy-element="{{.}}"{{/alchemy_id}}>
      {{{html}}}
    </div>
  {{/is_embed}}

  {{#is_image}}
    <div id="{{id}}" class="ve-base ve-base-image {{type}}{{#is_portrait}} portrait{{/is_portrait}}{{#is_landscape}} landscape{{/is_landscape}}"{{#alchemy_id}} data-alchemy-element="{{.}}"{{/alchemy_id}}>
     <figure>
       {{#image}}
         <picture>
          {{>atoms/images/responsive-srcset-picture}}
         </picture>
       {{/image}}
       {{#caption}}
          <figcaption>{{.}}</figcaption>
       {{/caption}}
     </figure>
    </div>
  {{/is_image}}

  {{#is_intro}}
    {{> organisms/virtual-exhibitions/intro-slide}}
  {{/is_intro}}

  {{#is_rich_image}}
    <div id="{{id}}" class="ve-base ve-base-title-image-text cf {{type}}"{{#alchemy_id}} data-alchemy-element="{{.}}"{{/alchemy_id}}{{#background_colour}} style="background-color:{{.}}"{{/background_colour}} {{^background_colour}}style="background-color:#ddebee"{{/background_colour}}>

      <div class="ve-title-group">
        {{#title}}
          <span class="ve-title">
            {{.}}
          </span>
        {{/title}}
        {{#subtitle}}
          <span class="ve-subtitle">
            {{{.}}}
          </span>
        {{/subtitle}}
      </div>

      <div class="ve-col-50-left">
        <div class="inner">
        {{#is_align_image_left}}
          {{#image}}
            <div class="ve-image">
              <figure>
                {{#image}}
                  <img src="{{#thumbnail}}{{url}}{{/thumbnail}}" {{#caption}}alt="{{.}}"{{/caption}}>
                {{/image}}
                {{#caption}}
                  <figcaption>{{.}}</figcaption>
                {{/caption}}
              </figure>
            </div>
          {{/image}}
        {{/is_align_image_left}}

        {{#is_align_image_right}}
           {{#body}}
             <div class="ve-rich-image-text">
               {{{.}}}
             </div>
           {{/body}}
           {{#quote}}
             <div id="{{id}}" class="ve-base-quote {{type}}">
               {{>atoms/text/quote}}
             </div>
          {{/quote}}
        {{/is_align_image_right}}
        </div>
      </div>

      <div class="ve-col-50-right">
        <div class="inner">
          {{#is_align_image_left}}
            {{#body}}
              <div class="ve-rich-image-text">
                {{{.}}}
              </div>
            {{/body}}
            {{#quote}}
              <div id="{{id}}" class="ve-base-quote {{type}}">
                {{>atoms/text/quote}}
              </div>
            {{/quote}}
          {{/is_align_image_left}}

          {{#is_align_image_right}}
            {{#image}}
              <div class="ve-image">
                <figure>
                  {{#image}}
                    <img src="{{#thumbnail}}{{url}}{{/thumbnail}}" {{#caption}}alt="{{.}}"{{/caption}}>
                  {{/image}}
                  {{#caption}}
                    <figcaption>{{.}}</figcaption>
                  {{/caption}}
                </figure>
              </div>
            {{/image}}
          {{/is_align_image_right}}
        </div>
      </div>
    </div>
  {{/is_rich_image}}

  {{^is_last}}
    <span class="slide-nav slide-nav-next"><a class="ve-anchor-link" href="#ve-anchor-{{id}}"><svg class="icon icon-down"><use xlink:href="#icon-down"></use></svg></a></span>
    <noscript>
      <a class="ve-anchor no-js" id="ve-anchor-{{id}}"></a>
    </noscript>
  {{/is_last}}
{{/is_section}}
