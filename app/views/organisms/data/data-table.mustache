<table class="data-table">
  <thead>
    <tr>
      <th><input type="checkbox" autocomplete="off"/></th>
      {{#head_data}}
        <th>{{.}}</th>
      {{/head_data}}
    </tr>
  </thead>
  <tbody>
    {{#row_data}}
      <tr{{#url}} class="linked-row"{{/url}}>
        {{#id}}<td><input type="checkbox" autocomplete="off" name="{{.}}"/></td>{{/id}}
        {{#cells}}
          <td>{{#url}}<a href="{{.}}">{{/url}}{{.}}{{#url}}</a>{{/url}}</td>
        {{/cells}}
      </tr>
    {{/row_data}}
  </tbody>
</table>
