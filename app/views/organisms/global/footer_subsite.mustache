{{# debug }}
{{! Debug template }}
    {{> atoms/utility/debug }}
{{/ debug }}

<!-- Start Footer -->
<footer class="footer" role="contentinfo">
    <div class="lc">
        <div class="grid">

            <div class="l-contact">
                <div class="footer_logo">
                    <a href="/"><img src="{{image_root}}europeana-logo.svg" alt="Europeana - think culture" /></a>
                </div>

            </div><!--

         --><div class="l-content">

                <div class="l-sitemap cf grid">
                {{# navigation.footer.linklist1 }}{{> molecules/navigation/linklist-with-title }}{{/ navigation.footer.linklist1 }}<!--
             -->{{# navigation.footer.linklist2 }}{{> molecules/navigation/linklist-with-title }}{{/ navigation.footer.linklist2 }}
                </div>

            </div><!--

         --><div class="l-nextsteps">
                {{# page_config.newsletter }}
                <div class="signup">
                    {{> molecules/forms/newsletter }}
                </div>
                {{/ page_config.newsletter }}

                {{# navigation.footer.social }}
                {{> molecules/navigation/social-links }}
                {{/ navigation.footer.social }}
            </div>
        </div>
    </div>

    <div class="lc">
        <h4 class="title">Powered by:</h4>
        <div class="l-supportedby">
        {{#listItems.three}}
            <div class="gi">
                {{> molecules-block-thumb }}
            </div>
        {{/listItems.three}}
        </div>
    </div>


    <div class="sub-footer lc">
        {{# navigation.footer.subfooter }}
        <ul class="nav">
            {{#items}}<li><a href="{{url}}">{{text}}</a></li>{{/items}}
        </ul>
        {{/ navigation.footer.subfooter }}

        <p class="copyright">Co-financed by the European Union.</p>
    </div>


</footer>
<!-- End Footer -->
