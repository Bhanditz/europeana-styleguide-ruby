{{#origin}}
  <div class="object-origin{{#institution_logo}} with-logo{{/institution_logo}}" {{#url}}data-object-origin-url="{{.}}"{{/url}} xmlns:cc="http://creativecommons.org/ns#" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:xhv="http://www.w3.org/1999/xhtml/vocab#">
    {{#institution_logo}}
      {{#url}}<a href="{{.}}" target="_blank" class="institution-logo" data-provided-by="{{i18n.site.object.meta-label.provided-by}}" style="background-image:url('{{institution_logo}}');" {{#institution_name}}aria-label="{{.}}"{{/institution_name}}></a>{{/url}}
      {{^url}}<span class="institution-logo" style="background-image:url('{{.}}');" {{#institution_name}}aria-label="{{.}}"{{/institution_name}}></span>{{/url}}
    {{/institution_logo}}
    {{#institution_name}}
      <span class="object-origin-detail">
        {{#url}}
          <span class="view-more-at">{{i18n.site.object.actions.view-more-at}}</span>
          <a class="link-black-blue header-only" data-provided-by="{{i18n.site.object.meta-label.provided-by}}" href="{{.}}" target="_blank">{{institution_name}}<span class="svg-icon-external"></span></a>
          <a class="link-black-blue overview-only" href="{{.}}" target="_blank">{{institution_name}}<span class="svg-icon-external"></span></a>
        {{/url}}
        {{^url}}{{institution_name}}{{/url}}
        <span resource="http://www.europeana.eu/rights/pd-usage-guide/" rel="cc:useGuidelines"></span>
      </span>
    {{/institution_name}}
  </div>
{{/origin}}
