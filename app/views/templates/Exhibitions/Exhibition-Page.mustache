{{> organisms/global/header_v2}}

<!-- Start content -->
<main role="main" class="l-page">

  <div class="page_header">
    {{>molecules/virtual-exhibitions/browse-breadcrumb.mustache}}
  </div>

  <div id="maincontent" class="maincontent">

  {{# page }}
    <div class="ve-slide-container">
      <span class="slide-nav slide-nav-prev">^</span>
      {{# elements.present }}
        <h4 class="ve-progress-nav">
          {{# elements.items }}
            <span class="ve-state-button {{#is_first}}ve-state-button-on ve-first{{/is_first}}{{^is_first}}ve-state-button-off{{/is_first}}"></span>
          {{/ elements.items }}
        </h4>
        {{# elements.items }}
          {{> organisms/virtual-exhibitions/main-slide}}
        {{/ elements.items }}
      {{/ elements.present }}
      <span class="slide-nav slide-nav-next">v</span>
      <a id="ve-end"/>
    </div>
  {{/ page }}

  <div class="lc">
    <h1>Foyer</h1>
    {{> organisms/virtual-exhibitions/foyer-card}}

    {{> organisms/virtual-exhibitions/foyer-card}}

    {{> organisms/virtual-exhibitions/foyer-card}}

    {{> organisms/virtual-exhibitions/foyer-card}}
  </div>
</div>

</main>
