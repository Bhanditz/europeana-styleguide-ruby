{{> atoms/iconography/svgicons-search }}
{{> atoms/iconography/svgicons-editing }}
{{> atoms/iconography/svgicons-user }}


{{> organisms/global/header_v2}}



<main role="main" class="l-page">
<div class="lc">

  <div class="l-content">

    <div class="page_header">
      {{> molecules/navigation/breadcrumbs}}
    </div>

    <div id="entry-detail" class="entry-detail">

      <div class="section">

        <h2>Add a New Project</h2>

        <div class="entry-upload entry-primary-upload">
        <form action="/file-upload">
          <div class="inner centre">

            <label for="heritagenew_videolink" class="required"><h3>Add the link to your entry.</h3> (enter a youTube, Vimeo, Slideshare or flickr link)</label>

            <input type="text" id="heritagenew_videolink" name="heritagenew[videolink]" required="required" placeholder="https://youtu.be/dQw4w9WgXcQ">

          </div>
        </form>
        </div>

        <div class="cf entry-info">
          <!-- Entry meta data -->
          <div class="entry-data">

            <div class="entry-upload entry-secondary-upload">
            <form action="/file-upload" class="dropzone">
              <div class="inner centre">

              <h3>Add a thumbnail image</h3>
              <input name="upload-placeholder" type="file" />

              </div>
            </form>
            </div>


            <div class="entry-data-inner">
              <ul>
                <li><a href="#" class="submitter-name editable" data-name="submitted-by" data-type="text" data-pk="1" data-title="Enter submitter name">{{ name.first }} {{ name.last }}</a> </li>

                <li><a href="#" class="editable" data-name="license" data-type="select" data-pk="1" data-title="Choose a license" data-source='[{value: "CC", text: "Creative commons"}, {value: "Other", text: "Other license"}]'>Choose a license</a></li>

                <li><a href="#" class="editable" data-name="submission-url" data-type="text" data-pk="1" data-title="Web address">{{ url }}</a> </li>
              </ul>
            </div>

          </div>


          <div class="entry-text prose">

            <h2><a href="#" class="editable" data-name="title" data-type="text" data-pk="1">Add a title here</a></h2>
            <div><a href="#" class="editable" data-name="description" data-type="textarea" data-pk="1">Add a description here</a></div>

          </div>
        </div>
      </div>


      <div class="item-meta-secondary section">
        <span class="item-tags">

        <svg class="icon icon-bookmark">
          <use xlink:href="#icon-bookmark"></use>
        </svg>

        <ul class="inline-list">
          {{# listItems.four }}
          <li><a href="{{ url }}">{{ name.first }}</a></li>
          {{/ listItems.four }}
          <li><a href="#" class="btn2 tag-add js-editable">Add a tag</a></li>
        </ul>

        </span>
      </div>


      <div class="extra-media section">
        <h3>Extra media</h3>
        <ul class="gallery_4">
          <li><a class="add-media" href="#"> + </a></li>
        </ul>
      </div>


      <div class="submitter-wrapper cf section">
        <h3>Submitted by:</h3>

        <h2 class="submitter-name"><a href="#" class="submitter-name2 editable" data-name="submitted-by2" data-type="text" data-pk="1" data-title="Enter submitter name">{{ name.first }} {{ name.last }}</a></h2>

        <div class="submitter-bio">
          {{ excerpt.short }}
        </div>

        <div class="submitter-meta">

          <div class="item-sendemail">
            <a class="email editable" href="#" data-name="submitted-email" data-type="text" data-pk="1" data-title="Enter submitter email">
              <svg class="icon icon-sendemail"><use xlink:href="#icon-sendemail"></use></svg>
              firsty.surnameson@europeana.eu
            </a>
          </div>

          <div class="item-phone-work">
            <svg class="icon icon-phone-work">
              <use xlink:href="#icon-phone-work"></use>
            </svg>

            <a class="tel editable" href="#" data-name="submitted-telephone" data-type="text" data-pk="1" data-title="Enter submitter phone number">+1.888.123.4567</a>

          </div>
        </div>
      </div>

    </div>




  </div>
  {{! END l-content }}


  <div class="l-sidebar">

    <div class="submit-buttons cf js-sticky">
      <div class="group1">
        <button class="save draft">
          <svg class="icon icon-check">
            <use xlink:href="#icon-check"></use>
          </svg>
          Save Draft</button>
      </div>

      <div class="group2">
        <button class="submit">Submit</button>
        <button class="cancel">Cancel</button>
      </div>
    </div>

  </div>
  {{! end sidebar }}

</div>
</main>
<!-- End Main -->


{{> organisms/global/footer_subsite }}

