{{> atoms/iconography/svgicons-pro}}

{{> organisms/global/header}}



<!-- Start content -->
<main role="main" class="l-page">
<div class="lc">
	<!-- Start grid -->
	<div class="grid-main">

		<div class="l-content">

			<div class="l-breadcrumbs">
				{{> molecules/navigation/breadcrumbs}}
			</div>

			<div id="maincontent" class="maincontent">

				<h1 class="title">{{ title }}</h1>


				<div class="elections">
					<h2>{{ subtitle }}</h2>

					<ul class="candidates-list">
						{{#listItems.eight}}<li>{{> molecules-person-block-wide(medium: true) }}</li>{{/listItems.eight}}
					</ul>
				</div>

			</div>
		</div><!--

	 --><div class="l-sidebar">
	 		<div class="inner">

				{{> molecules-help-contact(title:"Questions?") }}

			</div>

		</div>
	</div>
	<!-- End grid -->
</div>
</main>
<!-- End Content -->


{{> organisms/global/footer }}

