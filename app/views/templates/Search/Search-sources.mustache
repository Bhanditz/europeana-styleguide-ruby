{{> atoms/iconography/svgicons-search }}

{{> organisms/global/header_v2}}

<div role="main">

  {{>molecules/search/browse-breadcrumb.mustache}}

  <section class="static-header">
    <div class="lc">
      <h1 class="title">{{ content.title }}</h1>
      <p>{{ content.description }}</p>
    </div>
  </section>

  <section class="static-content section">
    <div class="lc">
      {{#content.providers}}
        {{>molecules/navigation/foldable-list}}
      {{/content.providers}}
      <div class="prose">
        {{{ content.text }}}
      </div>
    </div>
  </section>

</div>

