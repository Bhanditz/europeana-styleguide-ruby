      {{^items}}
        <section>
          <div class="object-media-info section is-current">
            <div class="object-downloads">
            </div>
          </div>
        </section>
      {{/items}}

    {{/object.media}}

    {{! Main object text info section }}
    <section class="section object-details">

      <div class="channel-object-overview">
        {{#object}}
          <div>
            {{#media}}

              <div class="object-media-wrap {{#empty_item}} empty{{/empty_item}}{{#single_item}}single-item{{/single_item}} {{^single_item}}{{^empty_item}}multi-item{{/empty_item}}{{/single_item}}">

                {{#title}}
                  <h2 class="channel-object-title top" property="http://purl.org/dc/elements/1.1/title dc:title">{{.}}</h2>
                {{/title}}

                <a class="scroll-trigger trigger-chain"
                   data-enabled="true"
                   data-disable-when-fired="showMediaThumbs"
                   data-fire-on-open-params='{ "trigger": "scroll-trigger-media-items"}'></a>

                {{#empty_item}}
                  <span class="no-preview">{{{empty_item_more_link}}}</span>
                {{/empty_item}}

                {{^empty_item}}
                  <div class="object-media-viewer">
                    {{#single_item}}
                      {{#items}}
                        <div class="single-item-thumb">
                          {{> molecules/components/media-nav-item}}
                        </div>
                      {{/items}}
                    {{/single_item}}

                    {{^single_item}}
                      {{#items}}
                        {{#is_current}}
                          <div class="media-poster{{#playable}} contains-playable{{/playable}}">
                            <div {{#playable}}class="playable"{{/playable}} {{#play_url}}data-uri="{{.}}"{{/play_url}}>
                              {{>molecules/components/thumb-icon}}
                              <span class="media-options">

                                <span class="media-option-group">
                                  <span class="media-option  media-share">
                                    <span class="share svg-icon-share"></span>
                                    <span class="media-option-label">Share</span>
                                  </span>
                                  <span class="media-option media-download">
                                    <span class="download svg-icon-download white"></span>
                                    <span class="media-option-label">Download</span>
                                  </span>
                                  <span class="media-option media-info">
                                    <span class="info svg-icon-info"></span>
                                    <span class="media-option-label">Info</span>
                                  </span>
                                </span>

                                <span class="media-option-group">
                                  <span class="media-option media-zoom-in">
                                    <span class="zoom-in svg-icon-zoom-in"></span>
                                    <span class="media-option-label">Zoom in</span>
                                  </span>
                                  <span class="media-option media-zoom-out">
                                    <span class="zoom-out svg-icon-zoom-out"></span>
                                    <span class="media-option-label">Zoom out</span>
                                  </span>
                                </span>

                                <span class="media-option-group">
                                  <span class="media-option  media-link">
                                    <span class="link svg-icon-link-out"></span>
                                    <span class="media-option-label">Link</span>
                                  </span>
                                </span>
                              </span>
                            </div>

                          </div>
                        {{/is_current}}
                      {{/items}}
                    {{/single_item}}

                    {{#required_players}}

                      {{#iiif}}
                        <div class="object-media-iiif is-hidden">
                          {{> molecules/components/iiif/iiif }}
                        </div>
                      {{/iiif}}

                      {{#video}}
                        <div class="object-media-video is-hidden">
                          {{> molecules/components/videojs/videojs }}
                        </div>
                      {{/video}}

                      {{#midi}}
                        <div class="object-media-midi is-hidden">
                          {{> molecules/components/midi/midi }}
                        </div>
                      {{/midi}}

                      {{#oembed}}
                        <div class="object-media-oembed is-hidden" data-fullscreen-enter="{{i18n.global.actions.fullscreen-enter}}" data-fullscreen-exit="{{i18n.global.actions.fullscreen-exit}}">
                          {{> molecules/components/oembed/oembed }}
                        </div>
                      {{/oembed}}

                      {{#text}}
                        <div class="object-media-text is-hidden">
                          Text viewer here
                        </div>
                      {{/text}}

                      {{#audio}}
                        <div class="object-media-audio is-hidden">
                         {{> molecules/components/videojs/videojs }}
                        </div>
                      {{/audio}}

                      {{#pdf}}
                        <div class="object-media-pdf is-hidden">
                          {{> molecules/components/pdfjs/pdfjs-ui-thumbs-right }}
                        </div>
                      {{/pdf}}
                    {{/required_players}}

                    <div class="title-desc-media-nav">

                      {{#title}}
                        <h2 class="channel-object-title" property="http://purl.org/dc/elements/1.1/title dc:title">{{.}}</h2>
                      {{/title}}

                      <div class="channel-object-media-nav">
                        {{^single_item}}
                          {{> molecules/components/media-nav}}
                        {{/single_item}}
                      </div>

                      {{#subtitle}}
                        <h3 class="subtitle">{{subtitle}}</h3>
                      {{/subtitle}}

                      <div class="channel-object-description">
                        {{#description}}
                          {{#sections}}
                            {{#items}}
                              <p>{{text}}</p>
                            {{/items}}
                          {{/sections}}
                        {{/description}}
                      </div>

                      <div class="channel-object-viewmore">
                        <a href="#" class="viewmore-image" style="background-image: url('/images/sample/object_thumbnail.jpg');"></a>
                        <h3>Johannes Vermeer</h3>
                        <a href="">View more</a>
                      </div>

                    </div>
                  </div>

                {{/empty_item}}

                {{#empty_item}}
                  {{#thumbnail}}<img src={{.}}>{{/thumbnail}}
                  {{^thumbnail}}<br/><br/>{{/thumbnail}}
                {{/empty_item}}

              </div>
            {{/media}}
          </div>

          <div class="channel-object-information channel-object-action-bar">

            <div class="action-ctrl">
              <button class="action-ctrl-btn share">{{i18n.global.actions.share}}</button>
            </div>

            <div class="action-ctrl">
              <button class="action-ctrl-btn download">{{i18n.site.object.actions.download}}</button>
            </div>

            {{#rights}}
              <div class="action-ctrl object-rights">
                <h4>{{ i18n.global.facet.header.reusability }}</h4>
                {{license_human}}
                <div class="object-license">{{> molecules/icons/licenses }}</div>
              </div>
            {{/rights}}

            <div class="action-ctrl">
              <button class="action-ctrl-btn annotate">{{i18n.site.object.actions.annotate}}</button>
            </div>

            {{#origin}}
              <div  style="margin: 0 0.8rem 0 auto; align-self: center;" class="object-origin" xmlns:cc="http://creativecommons.org/ns#" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:xhv="http://www.w3.org/1999/xhtml/vocab#">
                <h4>{{ i18n.site.object.meta-label.provided-by }}</h4>

                {{#institution_logo}}
                  <a href="{{url}}"><img class="institution-logo" src="{{.}}" alt="{{institution_name}} logo"></a>
                {{/institution_logo}}

                {{#logo}}
                  <a href="{{url}}"><img class="logo-institution" src="{{image_root}}{{logo}}" alt="{{institution_name}} logo"></a>
                {{/logo}}
                {{#institution_name_and_link}}
                  {{{ i18n.site.object.meta-label.provided-by-detail }}}
                {{/institution_name_and_link}}
                {{^institution_name_and_link}}
                  {{#institution_name}}
                    {{{ i18n.site.object.meta-label.provided-by-detail-no-link }}}
                    {{#disabled}}
                      <a href="{{institution_canned_search}}"
                         property="cc:attributionName"
                         rel="cc:attributionURL">{{institution_name}}</a>
                    {{/disabled}}
                    {{institution_name}}
                    <span resource="http://www.europeana.eu/rights/pd-usage-guide/" rel="cc:useGuidelines"></span>
                  {{/institution_name}}
                {{/institution_name_and_link}}
              </div>
            {{/origin}}

          </div>

          {{#information}}

            <div class="channel-object-information">
              {{#people}}
                {{> organisms/sections/object-data-section }}
              {{/people}}

              {{#concepts }}
                {{> organisms/sections/object-data-section }}
              {{/concepts }}
            </div>

          {{/information}}

          {{#extended_information}}

            <div class="channel-object-extended-information">

              {{#properties }}
                {{> organisms/sections/object-data-section }}
              {{/properties }}

              {{#dates }}
                {{> organisms/sections/object-data-section }}
              {{/dates }}

              {{#places }}
                {{> organisms/sections/object-data-section }}
              {{/places }}

              {{#annotations_later}}
                <script id="js-template-object-data-section" type="text/x-mustache">{{> organisms/sections/object-data-section-js }}</script>
              {{/annotations_later}}

              {{#annotations}}
                {{> organisms/sections/object-data-section }}
              {{/annotations}}

              {{#provenance}}
                {{> organisms/sections/object-data-section }}
              {{/provenance}}

              {{#copyright}}
                {{> organisms/sections/object-data-section }}
              {{/copyright}}

              {{#refs_rels}}
                {{> organisms/sections/object-data-section }}
              {{/refs_rels}}

              {{#meta_additional}}
                {{#present}}
                  <div class="data-section cf closed">
                    <h3 class="subsection-label">
                      {{i18n.site.object.meta-label.location}}
                    </h3>
                    <div class="subsection-content">
                      {{#places}}
                        <ul class="data-group">
                          {{#sections}}
                            <li>
                              <section>
                                <h4 class="data-header">{{#title }}{{.}}:{{/title }}</h4>
                                <ul class="comma-list data-group">
                                  {{# items }}
                                    <li>
                                      {{#url }}<a href="{{.}}">{{/url }}{{ text }}{{#url }}</a>{{/url }}{{#extra_info}}
                                        {{#life}}
                                          <span class="life-dates">
                                            ({{from.short}} {{#to.short}} â€” {{.}}{{/to.short}})
                                          </span>
                                        {{/life}}
                                        {{#biography}}
                                          <div class="biography">
                                            {{text}}
                                          </div>
                                        {{/biography}}
                                      {{/extra_info}}
                                    </li>
                                  {{/ items}}
                                </ul>
                              </section>
                            </li>
                          {{/sections}}
                        </ul>
                      {{/places}}

                      {{#geo.long_and_lat}} {{! check both present }}
                        <a class="scroll-trigger"
                           id="scroll-trigger-map"
                           data-enabled="true"
                           data-fire-on-open="showMap"
                           data-fire-on-open-params='{
                            "longitude":{{geo.longitude}}, "latitude": {{geo.latitude}}, "labels": { "n":"{{geo.labels.points.n}}", "s":"{{geo.labels.points.s}}", "e":"{{geo.labels.points.e}}", "w":"{{geo.labels.points.w}}" }
                           }'
                          ></a>
                        <span id="js-map-place-name" class="is-hidden">{{geo.placeName}}</span>
                        <a id="map" style="bottom:4.8em; position: relative;"></a>
                        <div class="map">
                          <div><b>{{geo.labels.longitude}}</b> {{#geo.longitude}}{{.}}{{/geo.longitude}}</div>
                          <div><b>{{geo.labels.latitude}}</b> {{#geo.latitude}}{{.}}{{/geo.latitude}}</div>
                        </div>
                        <a class="scroll-trigger trigger-chain"
                          data-enabled="true"
                          data-disable-when-fired="showMap"
                          data-fire-on-open-params='{"trigger": "scroll-trigger-map"}'></a>
                      {{/geo.long_and_lat}}
                    </div>
                  </div>
                {{/present}}
              {{/meta_additional}}

              <div class="data-section closed">
                <div class="hierarchy cf">
                  <div class="subsection-label">
                    {{i18n.site.object.hierarchy.title}}
                  </div>
                  <div class="subsection-content">
                    <div class="hierarchy-objects" style="display:none;"></div>
                  </div>
                </div>
              </div>

              {{#content.named_entities}}
                <div class="data-section closed">
                  <div class="cf">
                    <div class="subsection-label">
                      {{i18n.site.object.named-entities.title}}
                    </div>
                    <div class="subsection-content">
                      {{>molecules/navigation/foldable-list}}
                    </div>
                  </div>
                </div>
              {{/content.named_entities}}

            </div>
          {{/extended_information}}
        {{/object}}

      </div>

      {{! end of object info column }}

      <div class="channel-object-actions cf">
        <input type="hidden" name="js_edm_is_shown_by" value="{{data.edm_is_shown_by}}"/>
        {{# object.tags }}
          <div class="object-tags">
            <ul class="nav">
              {{#items}}
                <li><a href="{{url}}" class="tag">{{text}}</a></li>
              {{/items}}
            </ul>
          </div>
        {{/ object.tags }}

        {{#object.media}}
          {{#items}}
            {{#technical_metadata.colours}}
              {{> molecules/controls/colour-search }}
            {{/technical_metadata.colours}}
          {{/items}}
          {{> molecules/controls/colour-search-template }}
        {{/object.media}}

        {{# object.further_actions }}
          <div class="object-furtheractions">
            {{> molecules/controls/dropdown-menu }}
          </div>
        {{/ object.further_actions }}

        {{# object.social_share }}
          <div class="object-social">
            <h4>{{{ i18n.global.share }}}</h4>
            {{> molecules/controls/share-social }}
          </div>
        {{/ object.social_share }}
      </div>
      {{! end object actions }}


      <div class="data-full-w">

        {{#content.similar}}
          <div class="data-border js-hidden">
            <div class="data-section closed">
              <div class="mlt cf">
                <div class="subsection-label">
                  {{title}}
                </div>
                <div class="subsection-content">
                  <div class="js-carousel more-like-this">
                    <ul class="cf">
                      {{#items}}
                        <li class="js-carousel-item">
                          <div class="mlt-img-div height-to-width" style="background-image: url({{img.src}})">
                            <div class="inner"><a class="link" href="{{url}}">&nbsp;</a></div>
                          </div>
                          {{#title}}
                          <span class="js-carousel-title">
                            <a href="{{url}}">{{.}}</a>
                          </span>
                          {{/title}}
                        </li>
                      {{/items}}
                    </ul>
                  </div>
                </div>
              </div>
              <div class="show-more-mlt"{{^more_items_total_formatted}} style="display:none;"{{/more_items_total_formatted}}><a href="{{more_items_query}}">{{ i18n.site.object.show-more-similar-items }}â€¦ (<span class="number">{{more_items_total_formatted}}</span>)</a></div>
            </div>
          </div>
        {{/content.similar}}
      </div>
    </section>
  {{/content}}
</div>
{{> organisms/sections/banner }}
{{> organisms/global/footer_v2 }}
