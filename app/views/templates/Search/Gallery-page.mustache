{{> atoms/iconography/svgicons-search }}

{{> organisms/global/header_v2}}

<div role="main" class="page l-page">

  <section class="search-object">
    <div class="object-nav cf">
      <div class="object-nav-lists">
        <ul class="breadcrumbs">
          <li><a href="{{navigation.home_url}}"><svg class="icon icon-home"><use xlink:href="#icon-home"></use></svg>{{i18n.site.navigation.breadcrumb.return_home}}</a></li>
          <li><a href="{{content.galleries_link}}">{{i18n.global.galleries}}</a></li>
          <li><span class="lastitem"><strong>{{content.hero.title}}</strong></span></li>
        </ul>
      </div>
    </div>
  </section>

  <section class="site-hero gallery-foyer" id="maincontent" style="background-image:url('{{content.hero.url}}');">
    <div class="hero-text">
      <h1>{{content.hero.title}}</h1>
      <br/>
      <h3>{{content.hero.subtitle}}</h3>
    </div>
  </section>

  {{#content}}
    <section class="lc">
      <div class="gallery-shares">
        {{#social}}
          <h4>{{social_title}}</h4>
          {{>molecules/controls/share-social}}
        {{/social}}
      </div>
    </section>
  {{/content}}

</div>

{{> organisms/sections/banner }}
{{> organisms/global/footer_v2 }}
{{> molecules/forms/feedback }}
