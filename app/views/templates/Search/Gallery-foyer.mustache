{{> atoms/iconography/svgicons-search }}

{{> organisms/global/header_v2}}

{{#content}}
  <div role="main" class="page l-page">

    <section class="search-object">
      <div class="object-nav cf">
        <div class="object-nav-lists">
          <ul class="breadcrumbs">
            <li><a href="{{navigation.home_url}}"><svg class="icon icon-home"><use xlink:href="#icon-home"></use></svg>{{i18n.site.navigation.breadcrumb.return_home}}</a></li>
            <li><span class="lastitem"><strong>{{i18n.global.galleries}}</strong></span></li>
          </ul>
        </div>
      </div>
    </section>

    <section class="site-hero gallery-foyer" id="maincontent" style="background-image:url('{{content.hero.url}}');">
      <div class="hero-text">
        <h1>{{content.hero.title}}</h1>
        <br/>
        <h3>{{content.hero.subtitle}}</h3>
      </div>
    </section>

    <section class="lc">
      <div class="gallery-shares">
        {{#social}}
          <h4>{{social_title}}</h4>
          {{>molecules/controls/share-social}}
        {{/social}}
      </div>
    </section>

    <section class="lc">
      <div class="galleries">
        <div class="grid-sizer"></div>
        <div class="gutter-sizer"></div>
        {{#galleries}}
          <div class="image-set">
            <div class="cf">
              {{#images}}
                <a class="img-link" href="{{link}}?imgId={{url}}">
                  <img src="{{url}}"/>
                </a>
              {{/images}}
            </div>
            <div class="image-set-info">
              <span class="image-set-title">
                <a href="{{link}}">{{title}}</a>
              </span>
              <span class="image-set-data cf">
                {{count}} {{i18n.global.labels.images}}
                <span class="svg-icon-info"> </span>
              </span>
            </div>
          </div>
        {{/galleries}}
      </div>
    </section>

    <section class="lc">
      SOME MORE STUFF
    </section>

  {{/content}}
</div>


{{> organisms/sections/banner }}
{{> organisms/global/footer_v2 }}
{{> molecules/forms/feedback }}
